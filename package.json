{
  "name": "port",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://d3i-infra.github.io/dd-education",
  "dependencies": {
    "@tailwindcss/vite": "^4.1.12",
    "@testing-library/jest-dom": "^6.7.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.3.0",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "@visx/responsive": "^3.12.0",
    "@visx/scale": "^3.12.0",
    "@visx/text": "^3.12.0",
    "@visx/wordcloud": "^3.12.0",
    "concurrently": "^9.2.0",
    "lodash": "^4.17.21",
    "lottie-react": "^2.4.1",
    "react-highlight-words": "^0.21.0",
    "react-router-dom": "^7.8.1",
    "recharts": "^3.1.2",
    "typescript": "^5.9.2",
    "web-vitals": "^5.1.0",
    "zod": "^3.25.76"
  },
  "scripts": {
    "build:wheel": "cd src/framework/processing/py && poetry build --format wheel",
    "build:install-wheel": "cp -R src/framework/processing/py/dist/. ./public",
    "build:py": "npm run build:wheel && npm run build:install-wheel",
    "build:app": "vite build",
    "start:py": "nodemon --ext py --exec \"npm run build:py\"",
    "start": "concurrently 'npm run start:py' 'vite'",
    "build": "npm run build:py && npm run build:app",
    "archive": "cd build && rm -f release.zip && zip -r ../release.zip .",
    "release": "npm run build && npm run archive"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.16",
    "@types/lodash": "^4.17.20",
    "@types/react-highlight-words": "^0.20.0",
    "@vitejs/plugin-react": "^5.0.1",
    "autoprefixer": "^10.4.21",
    "husky": "^9.1.7",
    "nodemon": "^3.1.10",
    "postcss": "^8.5.6",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "tailwindcss": "^4.1.12",
    "ts-standard": "^12.0.2",
    "vite": "^7.1.3"
  },
  "ts-standard": {
    "project": "./tsconfig.json",
    "ignore": [
      "dist",
      "src/framework/processing/py_worker.js"
    ]
  }
}
